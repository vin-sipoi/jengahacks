# Staging Environment Variables
# Copy this file to .env.staging and fill in your staging values
# DO NOT commit .env.staging to Git

# Supabase Configuration (Staging Project)
VITE_SUPABASE_URL=https://<staging-project-ref>.supabase.co
VITE_SUPABASE_ANON_KEY=<staging-anon-key>

# reCAPTCHA (Use test keys for staging - always pass validation)
VITE_RECAPTCHA_SITE_KEY=6LeIxAcTAAAAAJcZVRqyHh71UMIEGNQ_MXjiZKhI

# Google Analytics (Optional - use separate property for staging)
VITE_GA_MEASUREMENT_ID=G-XXXXXXXXXX

# Feature Flags
VITE_USE_REGISTRATION_EDGE_FUNCTION=true

# Monitoring (Enable for staging, but disable alerts)
VITE_MONITORING_ENABLED=true
VITE_MONITORING_HEALTH=true
VITE_MONITORING_ALERTS=false
VITE_MONITORING_WEBHOOK_URL=

# Sentry (Use separate DSN for staging)
VITE_SENTRY_ENABLED=true
VITE_SENTRY_DSN=<staging-sentry-dsn>
VITE_SENTRY_ENVIRONMENT=staging

# Discord (Optional - can use same URL or separate staging channel)
VITE_DISCORD_URL=https://discord.gg/dU5g936a

# App Version (Optional)
VITE_APP_VERSION=staging
